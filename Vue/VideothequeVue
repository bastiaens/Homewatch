package homewatch;

import java.util.Observer;

//import homewatch.controleur.VideothequeController;
//import homewatch.modele.Videotheque;

public abstract class VideothequeVue implements Observer
{
	protected Videotheque modele;
	protected VideothequeController controleur;
	
	public VideothequeVue(Videotheque model, VideothequeController controller)
	{
		this.modele = model;
		this.controleur = controleur;
		
	}
	
	public abstract void affiche();
}
